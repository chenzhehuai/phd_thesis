      </bcf:map>
      <bcf:map map_overwrite="1">
        <bcf:map_step map_field_source="userf" map_match="^cn$" map_replace="zh"/>
      </bcf:map>
      <bcf:map>
        <bcf:map_step map_field_set="userf" map_field_value="en"/>
      </bcf:map>
    </bcf:maps>
    <bcf:maps datatype="bibtex" level="driver">
      <bcf:map>
        <bcf:map_step map_field_set="day" map_null="1"/>
